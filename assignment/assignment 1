-------- fifth database airline resvertion system
CREATE TABLE Flights (
    FlightID VARCHAR(10) PRIMARY KEY,
    Airline VARCHAR(50),
    Origin VARCHAR(50),
    Destination VARCHAR(50),
    Status VARCHAR(20)
);

INSERT INTO Flights VALUES
('FL1001', 'Delta Airlines', 'New York', 'London', 'On Time'),
('FL1002', 'Emirates', 'Dubai', 'Paris', 'Delayed'),
('FL1003', 'Lufthansa', 'Berlin', 'Tokyo', 'Cancelled'),
('FL1004', 'American Airlines', 'Chicago', 'Toronto', 'On Time'),
('FL1005', 'Qatar Airways', 'Doha', 'Sydney', 'On Time');

CREATE TABLE Passengers (
    PassengerID INT PRIMARY KEY,
    Name VARCHAR(100),
    PassportNumber VARCHAR(20),
    DOB DATE,
    Nationality VARCHAR(50)
);

INSERT INTO Passengers VALUES
(1, 'Emma Watson', 'A1234567', '1990-04-15', 'British'),
(2, 'Carlos Rivera', 'B7654321', '1985-09-22', 'Mexican'),
(3, 'Mei Lin', 'C3344556', '1992-12-03', 'Chinese'),
(4, 'James Patel', 'D5566778', '1988-06-30', 'Indian'),
(5, 'Anna Müller', 'E9988776', '1995-11-10', 'German');

CREATE TABLE Bookings (
    BookingID VARCHAR(10) PRIMARY KEY,
    PassengerID INT,
    FlightID VARCHAR(10),
    BookingDate DATE,
    Seat VARCHAR(5),
    FOREIGN KEY (PassengerID) REFERENCES Passengers(PassengerID),
    FOREIGN KEY (FlightID) REFERENCES Flights(FlightID)
);

INSERT INTO Bookings VALUES
('BK001', 1, 'FL1001', '2025-07-01', '12A'),
('BK002', 2, 'FL1002', '2025-07-02', '14B'),
('BK003', 3, 'FL1003', '2025-07-03', '22C'),
('BK004', 4, 'FL1004', '2025-07-04', '10D'),
('BK005', 5, 'FL1005', '2025-07-05', '16E');

CREATE TABLE Crew (
    CrewID INT PRIMARY KEY,
    Name VARCHAR(100),
    Role VARCHAR(50),
    AssignedFlightID VARCHAR(10),
    HireDate DATE,
    FOREIGN KEY (AssignedFlightID) REFERENCES Flights(FlightID)
);

INSERT INTO Crew VALUES
(201, 'Captain John Smith', 'Pilot', 'FL1001', '2015-03-12'),
(202, 'Mary Jones', 'Flight Attendant', 'FL1002', '2018-05-20'),
