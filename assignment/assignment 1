(204, 'Sarah Lee', 'Pilot', 'FL1004', '2016-11-09'),
(205, 'Liam O’Brien', 'Flight Attendant', 'FL1005', '2019-04-23');

CREATE TABLE Airports (
    AirportID VARCHAR(10) PRIMARY KEY,
    Name VARCHAR(100),
    City VARCHAR(50),
    Country VARCHAR(50),
    TimeZone VARCHAR(50)
);

INSERT INTO Airports VALUES
