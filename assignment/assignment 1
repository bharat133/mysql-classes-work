
-------------------------- create database with 30 database with 5 attributes --------------------------------------------------------------

-- for create database
create database bharat;

-- for use of database 
use bharat;

-- for drop the database 
drop database bharat;

-------------------------- 30  database tables with their 5 tables and 5 attributes  analysis------------------------------------------------------------------------
/*





1. Hospital Management System
Patients: PatientID, Name, DOB, Gender, Diagnosis
Doctors: DoctorID, Name, Specialty, YearsExperience, Department
Appointments: AppointmentID, PatientID, DoctorID, Date, Status
Prescriptions: PrescriptionID, PatientID, Medication, Dosage, StartDate
Billing: InvoiceID, PatientID, Amount, Date, Paid

2. University Academic System
Students: StudentID, Name, Major, EnrollmentYear, Email
Courses: CourseID, Title, Credits, Department, InstructorID
Enrollments: EnrollmentID, StudentID, CourseID, Semester, Grade
Faculty: FacultyID, Name, Department, Title, HireDate
Classrooms: RoomID, Building, Capacity, Type, Availability

3. ECommerce Platform
Customers: CustomerID, Name, Email, Phone, SignupDate
Products: ProductID, Name, Category, Price, Stock
Orders: OrderID, CustomerID, OrderDate, TotalAmount, Status
OrderItems: OrderItemID, OrderID, ProductID, Quantity, Price
Reviews: ReviewID, ProductID, CustomerID, Rating, Comment

4. Banking System
Accounts: AccountID, CustomerID, Balance, AccountType, OpenDate
Customers: CustomerID, Name, Address, SSN, DOB
Transactions: TransactionID, AccountID, Date, Type, Amount
Loans: LoanID, CustomerID, LoanType, Amount, StartDate
Branches: BranchID, Name, City, ManagerID, OpenDate

5. Airline Reservation System
Flights: FlightID, Airline, Origin, Destination, Status
Passengers: PassengerID, Name, PassportNumber, DOB, Nationality
Bookings: BookingID, PassengerID, FlightID, BookingDate, Seat
Crew: CrewID, Name, Role, AssignedFlightID, HireDate
Airports: AirportID, Name, City, Country, TimeZone

6. Hotel Management System
Guests: GuestID, Name, Email, Phone, LoyaltyMember
Reservations: ReservationID, GuestID, RoomID, CheckIn, Nights
Rooms: RoomID, Type, Rate, Floor, Status
Staff: StaffID, Name, Role, Department, HireDate
Payments: PaymentID, ReservationID, Amount, Method, Date

7. Retail Point‑of‑Sale System
Products: ProductID, Name, Price, Category, Stock
Sales: SaleID, Date, CashierID, Total, PaymentMethod
Customers: CustomerID, Name, Email, Phone, MemberSince
Cashiers: CashierID, Name, Shift, HireDate, ManagerID
Suppliers: SupplierID, Name, ProductCategory, ContactInfo, Rating

8. Logistics & Shipping
Shipments: ShipmentID, Origin, Destination, Weight, Status
Couriers: CourierID, Name, Region, Phone, Active
Packages: PackageID, ShipmentID, Weight, ContentType, Fragile
Warehouses: WarehouseID, City, Capacity, ManagerID, ISOCode
Routes: RouteID, Start, End, Distance, EstimatedTime

9. Insurance Claims Processing
Policies: PolicyID, CustomerID, Type, Premium, StartDate
Customers: CustomerID, Name, DOB, Address, Email
Claims: ClaimID, PolicyID, Date, ClaimType, Amount
Adjusters: AdjusterID, Name, Region, HireDate, LicenseNo
Payments: PaymentID, ClaimID, Amount, Date, Method

10. Media Streaming Service
Users: UserID, Name, Email, SubscriptionPlan, JoinDate
Titles: TitleID, Name, Genre, ReleaseYear, Rating
WatchHistory: WatchID, UserID, TitleID, WatchDate, Duration
Subscribers: SubscriberID, UserID, Plan, MonthlyFee, Status
Reviews: ReviewID, UserID, TitleID, Rating, Text

11. Social Media Platform
Users: UserID, Name, Username, Email, SignUpDate
Posts: PostID, UserID, Content, Timestamp, Likes
Comments: CommentID, PostID, UserID, CommentText, Timestamp
Followers: FollowerID, UserID, FollowerUserID, Since, Status
Messages: MessageID, FromUserID, ToUserID, MessageText, SentAt

12. IoT Smart Home System
Devices: DeviceID, Type, Status, Location, LastActive
Users: UserID, Name, Email, AuthLevel, CreatedAt
DeviceLogs: LogID, DeviceID, Timestamp, Action, Status
Scenes: SceneID, Name, Trigger, Actions, Active
Alerts: AlertID, DeviceID, AlertType, Timestamp, Resolved

13. Manufacturing ERP
Products: ProductID, Name, Category, UnitCost, Stock
Suppliers: SupplierID, Name, Contact, Rating, Country
PurchaseOrders: POID, SupplierID, Date, TotalAmount, Status
WorkOrders: WOID, ProductID, Quantity, StartDate, Status
Inventory: InventoryID, ProductID, WarehouseID, Quantity, LastUpdated

14. Real Estate Management
Properties: PropertyID, Address, Type, Bedrooms, Price
Owners: OwnerID, Name, Phone, Email, Address
Tenants: TenantID, Name, LeaseStart, LeaseEnd, MonthlyRent
Payments: PaymentID, TenantID, Amount, Date, Method
Inspections: InspectionID, PropertyID, InspectorID, Date, Status

15. Education LMS (Learning Management System)
Instructors: InstructorID, Name, Department, Email, HireDate
Courses: CourseID, Name, Credits, InstructorID, Term
Students: StudentID, Name, Major, Year, Email
Assignments: AssignmentID, CourseID, Title, DueDate, MaxScore
Submissions: SubmissionID, AssignmentID, StudentID, SubmitDate, Score

16. Event Ticketing Platform
Events: EventID, Name, VenueID, Date, Capacity
Venues: VenueID, Name, City, Capacity, Type
Attendees: AttendeeID, Name, Email, TicketType, CheckedIn
Tickets: TicketID, EventID, AttendeeID, Price, PurchaseDate
Staff: StaffID, EventID, Role, Name, Contact

17. Restaurant Reservation & Orders
Restaurants: RestaurantID, Name, Address, Cuisine, Rating
Tables: TableID, RestaurantID, Seats, Location, Status
Reservations: ResID, TableID, GuestName, Date, Time
Orders: OrderID, ResID, ItemID, Quantity, Price
MenuItems: ItemID, RestaurantID, Name, Price, Availability

18. Healthcare Insurance
Members: MemberID, Name, DOB, PlanType, JoinDate
Policies: PolicyID, MemberID, CoverageType, Premium, EffectiveDate
Claims: ClaimID, PolicyID, Date, Diagnosis, Amount
Providers: ProviderID, Name, Specialty, City, Rating
Visits: VisitID, MemberID, ProviderID, VisitDate, Reason

19. Sports League Management
Teams: TeamID, Name, City, CoachID, FoundedYear
Players: PlayerID, Name, TeamID, Position, JerseyNumber
Matches: MatchID, HomeTeamID, AwayTeamID, Date, Venue
Scores: ScoreID, MatchID, TeamID, Goals, Fouls
Referees: RefereeID, Name, ExperienceYears, Nationality, AssignedMatches

20. Car Rental System
Cars: CarID, Make, Model, Year, DailyRate
Customers: CustomerID, Name, LicenseNo, Phone, Email
Rentals: RentalID, CarID, CustomerID, StartDate, EndDate
Payments: PaymentID, RentalID, Amount, Date, Method
Maintenance: MaintID, CarID, Date, Description, Cost

21. Publishing House
Authors: AuthorID, Name, Country, DOB, Genre
Books: BookID, Title, AuthorID, PublishYear, ISBN
Sales: SaleID, BookID, DateSold, Quantity, TotalAmount
Editors: EditorID, Name, Specialty, BookID, HireDate
Contracts: ContractID, AuthorID, StartDate, EndDate, RoyaltyRate

22. Charity / NonProfit Organization
Donors: DonorID, Name, Email, Phone, JoinDate
Donations: DonationID, DonorID, Date, Amount, CampaignID
Campaigns: CampaignID, Title, StartDate, EndDate, GoalAmount
Volunteers: VolunteerID, Name, Availability, Skills, JoinedDate
Events: EventID, Name, Date, Location, AttendeesCount

23. Pharmaceutical Research
Studies: StudyID, Title, Phase, StartDate, PrincipalInvestigator
Patients: PatientID, StudyID, Age, Gender, EnrollmentDate
Medications: MedicationID, Name, Dosage, Manufacturer, ExpiryDate
Visits: VisitID, PatientID, VisitDate, Observations, AdverseEvents
Results: ResultID, VisitID, Measurement, Value, Unit

24. Music Streaming Analytics
Songs: SongID, Title, Artist, Genre, Duration
Listeners: ListenerID, Name, Country, SubscriptionType, JoinDate
Plays: PlayID, ListenerID, SongID, Timestamp, Device
Playlists: PlaylistID, ListenerID, Name, CreatedDate, SongCount
Likes: LikeID, ListenerID, SongID, Timestamp, Platform

25. Advertising Platform
Advertisers: AdvertiserID, Name, Industry, ContactEmail, Budget
Campaigns: CampaignID, AdvertiserID, StartDate, EndDate, Objective
Ads: AdID, CampaignID, Format, ImpressionCount, ClickCount
Platforms: PlatformID, Name, Type, AudienceSize, Region
Billing: BillID, AdvertiserID, Period, Amount, Paid

26. Film Production Studio
Films: FilmID, Title, Genre, ReleaseDate, Budget
Actors: ActorID, Name, DOB, Nationality, AgentID
Crew: CrewID, FilmID, Role, Name, ExperienceYears
Locations: LocationID, FilmID, City, Country, DurationDays
BoxOffice: BOID, FilmID, OpeningWeekend, TotalGross, Region

27. Legal Case Management
Clients: ClientID, Name, Address, Phone, Email
Cases: CaseID, ClientID, CaseType, StartDate, Status
Lawyers: LawyerID, Name, Specialty, HireDate, CasesHandled
Hearings: HearingID, CaseID, Date, Courtroom, Outcome
Invoices: InvoiceID, CaseID, IssueDate, Amount, Paid

28. Smart Agriculture / Farm IoT
Fields: FieldID, CropType, AreaAcres, IrrigationType, SoilType
Sensors: SensorID, FieldID, SensorType, LastCalibration, Status
Readings: ReadingID, SensorID, Timestamp, Value, Unit
Fertilizers: FertilizerID, Name, ApplicationRate, CostPerAcre, Supplier
Harvests: HarvestID, FieldID, Date, YieldKg, QualityGrade

29. Travel Booking Platform
Users: UserID, Name, Email, PreferredDestinations, JoinedDate
Trips: TripID, UserID, Destination, StartDate, EndDate
Accommodations: AccID, TripID, HotelName, CheckIn, Nights
Flights: FlightBookingID, TripID, Airline, FlightNumber, Date
Reviews: ReviewID, TripID, Rating, Comment, Date

30. Art Gallery Management
Artists: ArtistID, Name, Nationality, BirthYear, Style
Artworks: ArtworkID, Title, ArtistID, YearCreated, Medium
Exhibitions: ExhibitionID, Name, StartDate, EndDate, Venue
Sales: SaleID, ArtworkID, BuyerName, Date, SalePrice
Curators: CuratorID, Name, ExhibitionID, ExperienceYears, Specialty
*/


-- first databse of hospital mangement system
CREATE TABLE Patients (
    PatientID INT PRIMARY KEY,
    Name VARCHAR(100),
    DOB DATE,
    Gender VARCHAR(10),
    Diagnosis VARCHAR(255)
);

INSERT INTO Patients VALUES
(1, 'John Doe', '1980-03-15', 'Male', 'Diabetes'),
(2, 'Jane Smith', '1992-07-22', 'Female', 'Asthma'),
(3, 'Carlos Vega', '1975-11-05', 'Male', 'Hypertension'),
(4, 'Fatima Noor', '1988-02-10', 'Female', 'Fracture'),
(5, 'Mohammed Ali', '2000-09-30', 'Male', 'Migraine');

CREATE TABLE Doctors (
    DoctorID INT PRIMARY KEY,
    Name VARCHAR(100),
    Specialty VARCHAR(100),
    YearsExperience INT,
    Department VARCHAR(100)
);

INSERT INTO Doctors VALUES
(101, 'Dr. Alice Green', 'Cardiology', 12, 'Cardio Dept'),
(102, 'Dr. Rahul Singh', 'Orthopedics', 8, 'Ortho Dept'),
(103, 'Dr. Sarah Brown', 'Pediatrics', 5, 'Child Care'),
(104, 'Dr. Omar Youssef', 'Neurology', 15, 'Neuro Dept'),
(105, 'Dr. Emma Wang', 'Dermatology', 6, 'Skin Care');

CREATE TABLE Appointments (
    AppointmentID INT PRIMARY KEY,
    PatientID INT,
    DoctorID INT,
    Date DATE,
    Status VARCHAR(20),
    FOREIGN KEY (PatientID) REFERENCES Patients(PatientID),
    FOREIGN KEY (DoctorID) REFERENCES Doctors(DoctorID)
);

INSERT INTO Appointments VALUES
(1001, 1, 101, '2025-07-01', 'Completed'),
(1002, 2, 102, '2025-07-02', 'Scheduled'),
(1003, 3, 104, '2025-07-03', 'Completed'),
(1004, 4, 102, '2025-07-04', 'Cancelled'),
(1005, 5, 105, '2025-07-05', 'Scheduled');

CREATE TABLE Prescriptions (
    PrescriptionID INT PRIMARY KEY,
    PatientID INT,
    Medication VARCHAR(100),
    Dosage VARCHAR(50),
    StartDate DATE,
    FOREIGN KEY (PatientID) REFERENCES Patients(PatientID)
);

INSERT INTO Prescriptions VALUES
(5001, 1, 'Metformin', '500mg twice daily', '2025-07-01'),
(5002, 2, 'Ventolin', '2 puffs as needed', '2025-07-02'),
(5003, 3, 'Lisinopril', '10mg once daily', '2025-07-03'),
(5004, 4, 'Ibuprofen', '400mg as needed', '2025-07-04'),
(5005, 5, 'Sumatriptan', '50mg at onset', '2025-07-05');


CREATE TABLE Billing (
    InvoiceID INT PRIMARY KEY,
    PatientID INT,
    Amount DECIMAL(10, 2),
    Date DATE,
    Paid BOOLEAN,
    FOREIGN KEY (PatientID) REFERENCES Patients(PatientID)
);

INSERT INTO Billing VALUES
(9001, 1, 150.00, '2025-07-01', TRUE),
(9002, 2, 300.00, '2025-07-02', FALSE),
(9003, 3, 220.50, '2025-07-03', TRUE),
(9004, 4, 130.00, '2025-07-04', FALSE),
(9005, 5, 175.75, '2025-07-05', TRUE);

----- second database university accadmy system
CREATE TABLE Students (
    StudentID INT PRIMARY KEY,
    Name VARCHAR(100),
    Major VARCHAR(100),
    EnrollmentYear INT,
    Email VARCHAR(100)
);

INSERT INTO Students VALUES
(1001, 'Alice Johnson', 'Computer Science', 2022, 'alice.johnson@example.edu'),
(1002, 'Brian Chen', 'Mathematics', 2021, 'brian.chen@example.edu'),
(1003, 'Sophia Lee', 'Biology', 2023, 'sophia.lee@example.edu'),
(1004, 'David Kim', 'Economics', 2022, 'david.kim@example.edu'),
(1005, 'Nina Patel', 'History', 2020, 'nina.patel@example.edu');

CREATE TABLE Courses (
    CourseID VARCHAR(10) PRIMARY KEY,
    Title VARCHAR(100),
    Credits INT,
    Department VARCHAR(100),
    InstructorID INT
);

INSERT INTO Courses VALUES
('CS101', 'Intro to Programming', 3, 'Computer Science', 201),
('MATH201', 'Calculus II', 4, 'Mathematics', 202),
('BIO150', 'General Biology', 4, 'Biology', 203),
('ECON210', 'Microeconomics', 3, 'Economics', 204),
('HIST300', 'World History', 3, 'History', 205);

CREATE TABLE Enrollments (
    EnrollmentID INT PRIMARY KEY,
    StudentID INT,
    CourseID VARCHAR(10),
    Semester VARCHAR(10),
    Grade VARCHAR(2),
    FOREIGN KEY (StudentID) REFERENCES Students(StudentID),
    FOREIGN KEY (CourseID) REFERENCES Courses(CourseID)
);

INSERT INTO Enrollments VALUES
(1, 1001, 'CS101', 'Fall', 'A'),
(2, 1002, 'MATH201', 'Spring', 'B+'),
(3, 1003, 'BIO150', 'Fall', 'A-'),
(4, 1004, 'ECON210', 'Summer', 'B'),
(5, 1005, 'HIST300', 'Spring', 'A');

CREATE TABLE Faculty (
    FacultyID INT PRIMARY KEY,
    Name VARCHAR(100),
    Department VARCHAR(100),
    Title VARCHAR(50),
    HireDate DATE
);

INSERT INTO Faculty VALUES
(201, 'Dr. Emily Zhang', 'Computer Science', 'Associate Professor', '2017-08-15'),
(202, 'Dr. Mark Rivera', 'Mathematics', 'Professor', '2010-01-10'),
(203, 'Dr. Olivia Grant', 'Biology', 'Assistant Professor', '2019-09-01'),
(204, 'Dr. Henry Adams', 'Economics', 'Lecturer', '2015-03-21'),
(205, 'Dr. Aisha Malik', 'History', 'Professor', '2012-06-05');

CREATE TABLE Classrooms (
    RoomID VARCHAR(10) PRIMARY KEY,
    Building VARCHAR(50),
    Capacity INT,
    Type VARCHAR(30),
    Availability VARCHAR(10)
);

INSERT INTO Classrooms VALUES
('B101', 'Engineering Hall', 40, 'Lecture', 'Available'),
('C202', 'Science Center', 35, 'Lab', 'Occupied'),
('A303', 'Main Building', 25, 'Seminar', 'Available'),
('D404', 'Library Annex', 50, 'Lecture', 'Maintenance'),
('E505', 'Business School', 30, 'Computer Lab', 'Available');


----- third database ecommerce
CREATE TABLE Customers (
    CustomerID INT PRIMARY KEY,
    Name VARCHAR(100),
    Email VARCHAR(100),
    Phone VARCHAR(20),
    SignupDate DATE
);

INSERT INTO Customers VALUES
(1, 'Emily Carter', 'emily.carter@example.com', '555-1234', '2023-01-10'),
(2, 'Jake Thomas', 'jake.thomas@example.com', '555-2345', '2023-02-15'),
(3, 'Linda Nguyen', 'linda.nguyen@example.com', '555-3456', '2023-03-20'),
(4, 'Noah Smith', 'noah.smith@example.com', '555-4567', '2023-04-05'),
(5, 'Ava Brown', 'ava.brown@example.com', '555-5678', '2023-05-01');

CREATE TABLE Products (
    ProductID INT PRIMARY KEY,
    Name VARCHAR(100),
    Category VARCHAR(50),
    Price DECIMAL(10,2),
    Stock INT
);

INSERT INTO Products VALUES
(101, 'Wireless Headphones', 'Electronics', 79.99, 50),
(102, 'Running Shoes', 'Footwear', 59.99, 30),
(103, 'Smart Watch', 'Electronics', 129.99, 20),
(104, 'Coffee Maker', 'Home Appliances', 49.99, 25),
(105, 'Yoga Mat', 'Fitness', 19.99, 100);

CREATE TABLE Orders (
    OrderID INT PRIMARY KEY,
    CustomerID INT,
    OrderDate DATE,
    TotalAmount DECIMAL(10,2),
    Status VARCHAR(20),
    FOREIGN KEY (CustomerID) REFERENCES Customers(CustomerID)
);

INSERT INTO Orders VALUES
(5001, 1, '2023-06-01', 159.98, 'Shipped'),
(5002, 2, '2023-06-02', 59.99, 'Processing'),
(5003, 3, '2023-06-03', 129.99, 'Delivered'),
(5004, 4, '2023-06-04', 69.98, 'Cancelled'),
(5005, 5, '2023-06-05', 19.99, 'Delivered');

CREATE TABLE OrderItems (
    OrderItemID INT PRIMARY KEY,
    OrderID INT,
    ProductID INT,
    Quantity INT,
    Price DECIMAL(10,2),
    FOREIGN KEY (OrderID) REFERENCES Orders(OrderID),
    FOREIGN KEY (ProductID) REFERENCES Products(ProductID)
);

INSERT INTO OrderItems VALUES
(1, 5001, 101, 1, 79.99),
(2, 5001, 103, 1, 79.99),
(3, 5002, 102, 1, 59.99),
(4, 5003, 103, 1, 129.99),
(5, 5005, 105, 1, 19.99);

CREATE TABLE Reviews (
    ReviewID INT PRIMARY KEY,
    ProductID INT,
    CustomerID INT,
    Rating INT CHECK (Rating BETWEEN 1 AND 5),
    Comment TEXT,
    FOREIGN KEY (ProductID) REFERENCES Products(ProductID),
    FOREIGN KEY (CustomerID) REFERENCES Customers(CustomerID)
);

INSERT INTO Reviews VALUES
(9001, 101, 1, 5, 'Excellent sound quality!'),
(9002, 102, 2, 4, 'Very comfortable and fits well.'),
(9003, 103, 3, 3, 'Good features but battery life could be better.'),
(9004, 104, 4, 5, 'Makes great coffee!'),
(9005, 105, 5, 4, 'Good value for the price.');

----- fourth database banking system
CREATE TABLE Customers (
    CustomerID INT PRIMARY KEY,
    Name VARCHAR(100),
    Address VARCHAR(200),
    SSN VARCHAR(15),
    DOB DATE
);

INSERT INTO Customers VALUES
(1, 'John Anderson', '123 Main St, Springfield', '123-45-6789', '1980-04-12'),
(2, 'Emily Clark', '456 Elm St, Greenville', '987-65-4321', '1990-08-25'),
(3, 'Michael Brown', '789 Maple Ave, Lakeside', '555-66-7777', '1975-12-03'),
(4, 'Sophia Martinez', '321 Oak Dr, Rivertown', '222-33-4444', '1988-06-19'),
(5, 'David Wilson', '654 Pine Rd, Hillview', '111-22-3333', '1995-01-10');

CREATE TABLE Accounts (
    AccountID INT PRIMARY KEY,
    CustomerID INT,
    Balance DECIMAL(10,2),
    AccountType VARCHAR(20),
    OpenDate DATE,
    FOREIGN KEY (CustomerID) REFERENCES Customers(CustomerID)
);

INSERT INTO Accounts VALUES
(1001, 1, 1500.00, 'Checking', '2021-01-10'),
(1002, 2, 3200.75, 'Savings', '2020-06-15'),
(1003, 3, 500.00, 'Checking', '2022-09-20'),
(1004, 4, 2400.30, 'Savings', '2019-11-05'),
(1005, 5, 700.00, 'Checking', '2023-03-12');

CREATE TABLE Transactions (
    TransactionID INT PRIMARY KEY,
    AccountID INT,
    Date DATE,
    Type VARCHAR(20),
    Amount DECIMAL(10,2),
    FOREIGN KEY (AccountID) REFERENCES Accounts(AccountID)
);

INSERT INTO Transactions VALUES
(5001, 1001, '2023-07-01', 'Deposit', 300.00),
(5002, 1002, '2023-07-02', 'Withdrawal', 200.00),
(5003, 1003, '2023-07-03', 'Deposit', 150.00),
(5004, 1004, '2023-07-04', 'Transfer', 400.00),
(5005, 1005, '2023-07-05', 'Deposit', 250.00);

CREATE TABLE Loans (
    LoanID INT PRIMARY KEY,
    CustomerID INT,
    LoanType VARCHAR(30),
    Amount DECIMAL(10,2),
    StartDate DATE,
    FOREIGN KEY (CustomerID) REFERENCES Customers(CustomerID)
);

INSERT INTO Loans VALUES
(2001, 1, 'Home Loan', 250000.00, '2022-01-15'),
(2002, 2, 'Car Loan', 15000.00, '2023-03-10'),
(2003, 3, 'Personal Loan', 10000.00, '2021-07-20'),
(2004, 4, 'Education Loan', 30000.00, '2020-08-01'),
(2005, 5, 'Business Loan', 50000.00, '2023-05-05');

CREATE TABLE Branches (
    BranchID INT PRIMARY KEY,
    Name VARCHAR(100),
    City VARCHAR(50),
    ManagerID INT,
    OpenDate DATE
);

INSERT INTO Branches VALUES
