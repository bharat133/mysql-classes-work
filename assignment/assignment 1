CREATE TABLE Reservations (
    ReservationID INT PRIMARY KEY,
    GuestID INT,
    RoomID INT,
    CheckIn DATE,
    Nights INT,
    FOREIGN KEY (GuestID) REFERENCES Guests(GuestID)
);

