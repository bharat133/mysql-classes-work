-- table 5 Scientists
create table Scientists	(
ScientistID int , 
FullName varchar(100), 
DOB date, 
Gender varchar(100), 
Specialization varchar(100), 
Designation varchar(100), 
Department varchar(100), 
Email varchar(100),
CenterID int primary key ,
Status varchar(100)
 );
 
