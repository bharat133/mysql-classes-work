-- table 9 GroundControl
create table GroundControl	( 
ControlID int primary key, 
MissionID int , 
CenterID int , 
ContactPerson varchar(200), 
Phone varchar(10), 
Email varchar (100), 
UplinkFreq int , 
DownlinkFreq int, 
Location varchar(100), 
Status varchar(100),
foreign key (MissionID) references Missions (MissionID),
foreign key (CenterID) references Scientists  (CenterID)
);
