-- All table delete queries --
drop table Satellites;
-- all data delete Queries--
truncate table Satellites;
-- show the table 
select *from Satellites;

-- table 3 LaunchVehicles
create table LaunchVehicles	(
VehicleID int  primary key , 
Name varchar(100), 
Stages int, 
MaxPayload  int , 
RangeKM int , 
FuelType varchar(100), 
HeightM varchar(100), 
ThrustKN int, 
Status varchar(100), 
Manufacturer varchar(100)
);
