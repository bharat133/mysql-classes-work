-- table 11  Experiments
create table  Experiments(
ExperimentID int primary key , 
Name varchar (100), 
MissionID int, 
Objective varchar(100), 
Type varchar(100), 
PrincipalInvestigator varchar(100),
Institution varchar (100), 
Results varchar(100),
Duration datetime , 
Status varchar(100),
foreign key (MissionID) references Missions (MissionID)
);

