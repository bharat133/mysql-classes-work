-- table 7  ResearchCenters	
create table  ResearchCenters	(
CenterID int primary key , 
Name varchar(200), 
Location varchar(100), 
EstablishedYear year, 
Head varchar(100), 
Departments varchar (100), 
Area int , 
Contact varchar(15), 
Type varchar(100), 
Status varchar(100),
foreign key (CenterID) references Scientists  (CenterID)
);

