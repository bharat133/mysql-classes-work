 -- table 25 tableAnomalies	
create table Anomalies	(
AnomalyID int primary key,
 MissionID int, 
 Description varchar(100),
 DateDetected varchar(100),
 Severity varchar(100),
 Impact varchar(100),
 ResolvedBy varchar(100),
 ResolutionDate date,
 Notes varchar(100),
Status varchar(100),
foreign key (MissionId) references Missions(MissionId)
);

