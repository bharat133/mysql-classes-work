-- table 4 LaunchSites
create table LaunchSites(
SiteID int primary key , 
Name varchar(100), 
Location varchar(120), 
Capacity int , 
EstablishedYear year , 
Country varchar(100), 
Latitude  decimal(10,2), 
Longitude decimal(10,2), 
Area int , 
Status varchar (100)
);
