-- ------------------------------------------------------ Database Queries ---------------------------------------------------------------------------------------
-- create datbase isro  ---------------
create database isro ;


drop database isro ;


use isro;
-- ------------------------ database anylsis-------------------
/*

T1 : Missions (MissionID, MissionName, Type, Objective, LaunchDate, Status, VehicleID, Duration, Budget, Outcome),
T2 : Satellites	(SatelliteID, Name, Type, LaunchDate, OrbitType, Operator, Purpose, Mass, MissionID, Status),
T3 : LaunchVehicles	(VehicleID, Name, Stages, MaxPayload, RangeKM, FuelType, HeightM, ThrustKN, Status, Manufacturer),
T4 : LaunchSites	(SiteID, Name, Location, Capacity, EstablishedYear, Country, Latitude, Longitude, Area, Status),
T5 : LaunchEvents	(EventID, MissionID, VehicleID, SiteID, LaunchDate, Success, Duration, AltitudeKM, Notes, Status),
T6 : Scientists	(ScientistID, FullName, DOB, Gender, Specialization, Designation, Department, Email, CenterID, Status),
T7	: ResearchCenters	(CenterID, Name, Location, EstablishedYear, Head, Departments, Area, Contact, Type, Status),
T8	: TelemetryStations	(StationID, Name, Location, FrequencyBand, SupportedVehicles, Country, Contact, CoverageArea, Operator, Status),
T9	: GroundControl	(ControlID, MissionID, CenterID, ContactPerson, Phone, Email, UplinkFreq, DownlinkFreq, Location, Status),
T10	: SpacecraftComponents	(ComponentID, Name, Type, Manufacturer, Weight, PowerUsage, Material, MissionID, LaunchDate, Status),
T11	: Experiments	(ExperimentID, Name, MissionID, Objective, Type, PrincipalInvestigator, Institution, Results, Duration, Status),
T12	: Departments	(DepartmentID, Name, CenterID, Head, Employees, Budget, Projects, Field, Contact, Status),
T13	: ISROEmployees	(EmployeeID, Name, Gender, DOB, DepartmentID, Designation, Salary, CenterID, Contact, Status),
T14	: Collaborations	(CollabID, PartnerName, Country, MissionID, Purpose, AgreementDate, EndDate, Contact, BudgetShare, Status),
T15	: SatellitesInOrbit	(OrbitID, SatelliteID, OrbitType, Inclination, AltitudeKM, PeriodMinutes, Coverage, SpeedKMPS, LaunchDate, Status),
T16	: Payloads	(PayloadID, Name, SatelliteID, Type, Weight, PowerConsumption, Manufacturer, Objective, CenterID, Status),
T17	: RocketTests	(TestID, VehicleID, Date, Location, Result, FuelUsed, Stage, MaxAltitude, Temperature, Status),
T18	: WeatherMonitoring	(RecordID, StationID, DateTime, Temperature, Humidity, WindSpeed, Pressure, DataQuality, Location, Status),
T19	: DataCenters	(DataCenterID, Name, Location, CapacityTB, EstablishedYear, Head, Services, Contact, Type, Status),
T20	: FundingRecords	(FundID, MissionID, Source, Amount, ReleaseDate, Purpose, DepartmentID, ApprovedBy, Utilization, Status),
T21	: TrainingPrograms (ProgramID, Name, Type, DurationDays, DepartmentID, StartDate, EndDate, Trainer, Location, Status),
T22	: InternationalProjects	(ProjectID, Name, PartnerAgency, Country, Objective, StartDate, EndDate, Budget, Output, Status),
T23	: SatelliteOperations (OperationID, SatelliteID, OperationType, Date, Duration, SignalStrength, Anomaly, FixApplied, ControllerID, Status),
T24	: MissionReports	(ReportID, MissionID, Title, Date, Summary, Author, DocumentURL, PageCount, ApprovalStatus, Status),
T25	: Anomalies	(AnomalyID, MissionID, Description, DateDetected, Severity, Impact, ResolvedBy, ResolutionDate, Notes, Status),
T26	: AcademicPartners	(PartnerID, UniversityName, ContactPerson, Country, MOUDate, AreaOfWork, Projects, Budget, Duration, Status),
T27	: Patents	(PatentID, Title, Inventor, FilingDate, GrantDate, Country, CenterID, ApplicationNo, Status, Field),
T28	: InnovationProjects	(InnovationID, Title, Description, Initiator, StartDate, EndDate, Budget, DepartmentID, CenterID, Status),
T29	: Astronauts	(AstronautID, Name, DOB, Gender, Nationality, SelectionDate, Missions, TrainingCenter, Status, Background),
T30	: SafetyInspections(InspectionID, SiteID, Date, Inspector, Department, Findings, Recommendations, ComplianceDate, Score, Status)
*/
--   ------------------------------------ tables Queires ----------------------------
-- table 1 Missions
create table Missions (
MissionID int primary key ,
MissionName varchar (100),
Type varchar(50),
Objective varchar(100),
LaunchDate date, 
Status varchar (100),
VehicleID int ,
Duration datetime, 
Budget varchar (100), 
Outcome varchar(100),
foreign key (VehicleID) references LaunchVehicles (VehicleID)
);
-- insert in values Mission table
insert into Missions
values 
(1, 'Chandrayaan-1', 'Lunar', 'Explore Moon surface', '2008-10-22', 'Completed', 101, '2008-10-22 00:00:00', 386000000, 'Successful'),

(2, 'Mangalyaan', 'Mars Orbiter', 'Mars atmospheric study', '2013-11-05', 'Completed', 102, '2014-09-24 00:00:00', 450000000, 'Successful'),

(3, 'Chandrayaan-2', 'Lunar', 'Soft landing on Moon', '2019-07-22', 'Partial Success', 103, '2019-09-07 00:00:00', 978000000, 'Orbiter Successful'),

(4, 'Gaganyaan', 'Crewed', 'Manned spaceflight mission', '2025-01-01', 'Planned', 104, NULL, 9020000000, 'In Progress'),

(5, 'RISAT-1', 'Earth Observation', 'All-weather surveillance', '2012-04-26', 'Completed', 105, '2012-04-26 00:00:00', 600000000, 'Successful'),

(6, 'INSAT-3DR', 'Meteorological', 'Weather monitoring', '2016-09-08', 'Active', 106, '2016-09-08 00:00:00', 950000000, 'Operational'),

(7, 'GSAT-29', 'Communication', 'High-throughput satellite', '2018-11-14', 'Active', 107, '2018-11-14 00:00:00', 790000000, 'Operational'),

(8, 'PSLV-C51', 'Launch Service', 'Commercial satellite deployment', '2021-02-28', 'Completed', 108, '2021-02-28 00:00:00', 320000000, 'Successful'),

(9, 'Aditya-L1', 'Solar', 'Solar corona observation', '2023-09-02', 'Active', 109, '2023-09-02 00:00:00', 378000000, 'Operational'),

(10, 'Shukrayaan-1', 'Venus Orbiter', 'Study Venus atmosphere', '2028-12-01', 'Planned', 110, NULL, 5000000000, 'Scheduled');
-- All table delete queries --
drop table Missions;
-- all data delete Queries--
truncate table Missions;
-- show the table 
select *from Missions;

-- table 2 Satellites
create table Satellites	(
SatelliteID int primary key ,
Name varchar(50),
Type varchar(50), 
LaunchDate date , 
OrbitType varchar(100), 
Operator varchar (100), 
Purpose varchar (100), 
Mass varchar (100), 
MissionID int ,
Status varchar(100),
foreign key (MissionId) references Missions(MissionId)
);
-- insert in values Satellites table
insert into Satellites
